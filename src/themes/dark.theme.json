{
  "nav": {
    "container": {
      "position": "fixed",
      "display": "flex",
      "backgroundColor": "#111111",
      "width": "100%",
      "height": "40px",
      "top": "0px",
      "left": "0px",
      "zIndex": "1",
      "alignItems": "center",
      "justifyContent": "space-between"
    },
    "item": {
      "height": "35px",
      "color": "#F8F4EC"
    }
  },
  "home": {
    "container": {
      "display": "block",
      "height": "calc(100vh - 115px)",
      "paddingTop": "40px",
      "paddingLeft": "15px",
      "paddingRight": "15px",
      "paddingBottom": "15px",
      "backgroundColor": "#555555"
    }
  },
  "footer": {
    "container": {
      "display": "flex",
      "height": "60px",
      "backgroundColor": "#444444",
      "color": "#F8F4EC",
      "justifyContent": "center",
      "alignItems": "center"
    }
  }
}